#include<iostream>
using namespace std;

int main()
{
int Arr[10];
for(int i=0;i<10;i++)
{
cout<<"Enter element"<<i+1<<endl;
cin>>Arr[i];
}
int d=Arr[0];
int s=0,e=9;
int mid=s+(e-s)/2;
while(s<e)
{
if(Arr[mid]-mid==d)
{
s=mid+1;
}
else if(Arr[mid]-mid==d+1)
{
e=mid;
}
mid=s+(e-s)/2;
}
cout<<"missing term is â€œ<<Arr[mid]-1<<endl;
Return 0;
}

/* input - 
Enter element1: 1
Enter element2: 2
Enter element3: 3
Enter element4: 4
Enter element5: 6
Enter element6: 7
Enter element7: 8
Enter element8: 9
Enter element9: 10
Enter element10: 11

output - 
missing term is 5
/*
