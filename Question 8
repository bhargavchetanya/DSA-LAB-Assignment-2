#include <iostream>
using namespace std;
int main(){
    int n;
    cin>>n;
    int a[n],c=0;
    for(int i=0;i<n;i++) 
    cin>>a[i];
    for(int i=0;i<n;i++){
        bool d=0;
        for(int j=0;j<i;j++){
            if(a[i]==a[j]){ 
            d=1; 
            break; 
            }
        }
        if(!d) 
        c++;
    }
    cout<<c;
}

